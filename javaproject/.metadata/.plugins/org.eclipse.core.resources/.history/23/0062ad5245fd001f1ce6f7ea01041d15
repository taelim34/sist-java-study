package day0310;


//스레드가 아닌상태에서의 실행결과
public class ThredEx1 {
	
	
	String name;
	int num;
	
	public ThredEx1(String name, int num) {
		this.name=name;
		this.num=num;
	}
	
	
	public void num()
	{
		for(int i=1;i<=num;i++)
		{
			System.out.println(name+"==>"+i);
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
