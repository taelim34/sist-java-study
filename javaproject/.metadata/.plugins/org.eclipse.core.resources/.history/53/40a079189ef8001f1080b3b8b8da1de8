package day0304;

class Point{
	
	int x;
	int y;
	//디폴트 생성자
	public Point() {
		// TODO Auto-generated constructor stub
	}
	
	//명시적 생성자
	public Point(int x, int y) {

		this.x=x;
		this.y=y;
		
	}
}

