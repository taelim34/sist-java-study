package day0228;

class MyBeg{
	String bagName;
	int bagPrice;
	String bagColor;
}
///////////////////////////////////////////////
public class Ex6CallByRef {

	//입력
	public static void bagInfoIn(MyBeg bag)
	{
		//MyBag멤버 초기화
		bag.bagName = "백팩";
		bag.bagPrice=65000;
		bag.bagColor="블랙";
		  
		
		
	}
	//출력
	public static void bagInfoOut(MyBeg bag) {
		
		System.out.println("가방명: "+bag.bagName);
		System.out.println("가방가격: "+bag.bagPrice);
		System.out.println("가방색상: "+bag.bagColor);
		
	}
	
	
	public static void main(String[] args) {
		// 두 메서드 다 주소전달되므로 메인의 bag변수에 데이터가 들어가고 출력된다
		bagInfo(null);

	}

}
