package day0227;

public class QuizShop {
	
	
	static String storeName;
	static int cnt=0;
	private String name;
	private int amount;
	private int price;
	
	cnt++;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}

}
