package day0310;

public class ThreadEx3 implements Runnable{
	
	String name;
	int num;
	
	public ThreadEx3(String name, int num) {
		this.name=name;
		this.num=num;
	}

	@Override
	public void run() {
		for(int i=1;i<=num;i++)
		{
			System.out.println(name+"==>"+i);
			
			Thread.sleep(300);
		}
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		

	}

	
}
