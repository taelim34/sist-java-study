package day0304;

import javax.swing.JFrame;

public class Ex5Frame extends JFrame{
	
	//생성자
	public Ex5Frame() {
		super("프레임연습");
		this.setBounds(300, 1000, 500, 500);
		this.setVisible(true);
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new Ex5Frame();
		
	}

}
