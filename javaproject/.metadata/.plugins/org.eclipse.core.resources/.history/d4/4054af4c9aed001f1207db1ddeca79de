package day2018;

public class DataType_05 {

	public static void main(String[] args) {
		// 기본자료형 8가지
		
		
		//1.논리형, true,false
		boolean flag=false;
		
		//2.문자형
		//반드시 하나의 문자를 가지며 2바인트: c언어는 1바이트라 한글표현 불가능
		//기본적으로 정수형,아스키코드로 변경 가능하다... ' '(소따음표)로 표현
		char ch='가';
		
		//3.정수형
		byte a=-128;//1바이트 -128~127까지 가능
		short b=32000;//
		int c=670000;
		long num=100l;

	}

}
